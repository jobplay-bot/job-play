{
  "name": "study-nest",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "echo=${APP_NAME} && nest build ${APP_NAME} -b webpack",
    "start": "echo=${APP_NAME} && node _output/dist/${APP_NAME}/index.js",
    "start:dev": "nest start ${APP_NAME} --watch",
    "start:debug": "nest start ${APP_NAME} --debug --watch",
    "format": "prettier --write \"src/apps/**/*.ts\" \"src/libs/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --watch --config jest.config.ts",
    "test:cov": "jest --coverage --config jest.config.ts",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/study-nest/test/jest-e2e.json",
    "migration:create": "npx mikro-orm migration:create",
    "migration:up": "npx mikro-orm migration:up"
  },
  "APP_NAME": "applications cores",
  "TEST_NAME": "all apps common",
  "dependencies": {
    "@mikro-orm/core": "^6.4.2",
    "@mikro-orm/migrations": "^6.4.2",
    "@mikro-orm/mysql": "^6.4.2",
    "@mikro-orm/nestjs": "^6.0.2",
    "@mikro-orm/reflection": "^6.4.2",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.55.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.57.0",
    "@opentelemetry/instrumentation-nestjs-core": "^0.44.0",
    "@opentelemetry/sdk-node": "^0.57.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "joi": "^17.13.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.4.2",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "jest-plugin-context": "^2.9.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.3.2"
  } 
}